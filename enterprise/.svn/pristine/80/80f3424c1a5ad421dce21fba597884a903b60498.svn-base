@charset "UTF-8";
/*add button*/		
.flexigrid div.fbutton .add {
	background: url(images/add.png) no-repeat center left;
}	

/*delete button*/		
.flexigrid div.fbutton .delete {
	background: url(images/close.png) no-repeat center left;
}	

/*edit button*/		
.flexigrid div.fbutton .edit {
	background: url(images/edit.png) no-repeat center left;
}

/*xls button*/		
.flexigrid div.fbutton .xls {
	background: url(images/tool_xls.gif) no-repeat center left;
}

/*csv button*/		
.flexigrid div.fbutton .csv {
	background: url(images/tool_csv.gif) no-repeat center left;
}

/*pdf button*/		
.flexigrid div.fbutton .pdf {
	background: url(images/tool_pdf.gif) no-repeat center left;
}

/*chart button*/		
.flexigrid div.fbutton .chart {
	background: url(images/tool_chart.gif) no-repeat center left;
}

/*print button*/		
.flexigrid div.fbutton .print {
	background: url(images/tool_print.gif) no-repeat center left;
}

/* CSS Document */
html { 
	overflow:-moz-scrollbars-vertical;
	overflow-y:scroll;
}

.flexigrid {
	font-family: Arial, Helvetica, sans-serif;
	font-size: 12px;
	position: relative;
	border: 0px solid #eee;
	overflow: hidden;
	color: #000;
}

.flexigrid.hideBody {
	font-size: 12px;
	height: 24px !important;
	border-bottom: 1px solid #d0d0d0;
}

.ie6fullwidthbug {
	font-size: 12px;
	border-right: 0px solid #d0d0d0;
	padding-right: 2px;
}

.flexigrid div.nDiv {
	font-size: 12px;
	background: #eee url(images/line.gif) repeat-y -1px top;
	border: 1px solid #d0d0d0;
	border-top: 0px;
	overflow: auto;
	left: 0px;
	position: absolute;
	z-index: 999;
	float: left;
}

.flexigrid div.nDiv table {
	font-size: 12px;
	margin: 2px;
}

.flexigrid div.hDivBox {
	font-size: 12px;
	float: left;
	padding-right: 40px;
}

.flexigrid div.bDiv table {
	font-size: 12px;
	margin-bottom: 10px;
}

.flexigrid div.bDiv table.autoht {
	font-size: 12px;
	border-bottom: 0px;
	margin-bottom: 0px;
}

.flexigrid div.nDiv td {
	font-size: 12px;
	padding: 2px 3px;
	border: 1px solid #eee;
	cursor: default;
}

.flexigrid div.nDiv tr:hover td,.flexigrid div.nDiv tr.ndcolover td {
	font-size: 12px;
	background: #d5effc url(images/hl.png) repeat-x top;
	border: 1px solid #a8d8eb;
}

.flexigrid div.nDiv td.ndcol1 {
	font-size: 12px;
	border-right: 1px solid #d0d0d0;
}

.flexigrid div.nDiv td.ndcol2 {
	font-size: 12px;
	border-left: 1px solid #fff;
	padding-right: 10px;
}

.flexigrid div.nDiv tr:hover td.ndcol1,.flexigrid div.nDiv tr.ndcolover td.ndcol1 {
	font-size: 12px;
	border-right: 1px solid #d2e3ec;
}

.flexigrid div.nDiv tr:hover td.ndcol2,.flexigrid div.nDiv tr.ndcolover td.ndcol2 {
	font-size: 12px;
	border-left: 1px solid #eef8ff;
}

.flexigrid div.nBtn {
	font-size: 12px;
	position: absolute;
	height: 24px;
	width: 14px;
	z-index: 900;
	background: #fafafa url(images/fhbg.gif) repeat-x bottom;
	border: 0px solid #d0d0d0;
	border-left: 1px solid #d0d0d0;
	top: 0px;
	left: 0px;
	margin-top: 1px;
	cursor: pointer;
	display: none;
}

.flexigrid div.nBtn div {
	font-size: 12px;
	height: 24px;
	width: 12px;
	border-left: 1px solid #fff;
	float: left;
	background: url(images/ddn.png) no-repeat center;
}

.flexigrid div.nBtn.srtd {
	font-size: 12px;
	background: url(images/wbg.gif) repeat-x 0px -1px;
}

.flexigrid div.mDiv {
	font-size: 12px;
	background: url(images/wbg.gif) repeat-x top;
	border: 1px solid #d0d0d0;
	border-bottom: 0px;
	border-top: 0px;
	font-weight: bold;
	display: block;
	overflow: hidden;
	white-space: nowrap;
	position: relative;
}

.flexigrid div.mDiv div {
	font-size: 12px;
	padding: 6px;
	white-space: nowrap;
}

.flexigrid div.mDiv div.ptogtitle {
	position: absolute;
	top: 4px;
	right: 3px;
	padding: 0px;
	height: 16px;
	width: 16px;
	overflow: hidden;
	border: 1px solid #d0d0d0;
	cursor: pointer;
	font-size: 12px;
}

.flexigrid div.mDiv div.ptogtitle:hover {
	background-position: left -2px;
	border-color: #bbb;
	font-size: 12px;
}

.flexigrid div.mDiv div.ptogtitle span {
	display: block;
	border-left: 1px solid #eee;
	border-top: 1px solid #fff;
	border-bottom: 1px solid #d0d0d0;
	width: 14px;
	height: 14px;
	background: url(images/uup.png) no-repeat center;
	font-size: 12px;
}

.flexigrid div.mDiv div.ptogtitle.vsble span {
	background: url(images/ddn.png) no-repeat center;
	font-size: 12px;
}

.flexigrid div.tDiv /*toolbar*/ {
	background: #fafafa url(images/bg.gif) repeat-x top;
	/*position: relative;*/
	border: 1px solid #d0d0d0;
	border-bottom: 0px;
	overflow: hidden;
	font-size: 12px;
	padding: 1px;
}

.flexigrid div.tDiv2 {
	float: left;
	clear: both;
	/*padding: 1px;*/
	font-size: 12px;
}

.flexigrid div.sDiv /*toolbar*/ {
	background: #fafafa url(images/bg.gif) repeat-x top;
	position: relative;
	border: 1px solid #d0d0d0;
	border-top: 0px;
	overflow: hidden;
	display: none;
	font-size: 12px;
}

.flexigrid div.sDiv2 {
	float: left;
	clear: both;
	padding: 5px;
	padding-left: 5px;
	width: 1000px;
	font-size: 12px;
}

.flexigrid div.sDiv2 input,.flexigrid div.sDiv2 select {
	vertical-align: middle;
	font-size: 12px;
}

.flexigrid div.btnseparator {
	float: left;
	height: 24px;
	border-left: 1px solid #d0d0d0;
	border-right: 1px solid #fff;
	margin: 1px;
	font-size: 12px;
}

.flexigrid div.fbutton {
	float: left;
	display: block;
	cursor: pointer;
	padding: 1px;
	font-size: 12px;
}

.flexigrid div.fbutton div {
	float: left;
	padding: 1px 3px;
	font-size: 12px;
}

.flexigrid div.fbutton span {
	float: left;
	display: block;
	padding: 3px;
	font-size: 12px;
}

.flexigrid div.fbutton:hover,.flexigrid div.fbutton.fbOver {
	padding: 0px;
	border: 1px solid #d0d0d0;
	font-size: 12px;
}

.flexigrid div.fbutton:hover div,.flexigrid div.fbutton.fbOver div {
	padding: 0px 2px;
	border-left: 1px solid #fff;
	border-top: 1px solid #fff;
	border-right: 1px solid #eee;
	border-bottom: 1px solid #eee;
	font-size: 12px;
}

/* end toolbar*/
.flexigrid div.hDiv {
	font-size: 12px;
	background: #fafafa url(images/fhbg.gif) repeat-x bottom;
	position: relative;
	height: 26px;
	border: 1px solid #d0d0d0;
	border-bottom: 0px;
	overflow: hidden;
	background: #fafafa url(images/fhbg.gif) repeat-x bottom;
}

.flexigrid div.hDiv table {
	border-right: 1px solid #fff;
	font-size: 12px;
}

.flexigrid div.cDrag {
	float: left;
	position: absolute;
	z-index: 2;
	overflow: visible;
	font-size: 12px;
}

.flexigrid div.cDrag div {
	float: left;
	background: none;
	display: block;
	position: absolute;
	height: 24px;
	width: 5px;
	cursor: col-resize;
	font-size: 12px;
}

.flexigrid div.cDrag div:hover,.flexigrid div.cDrag div.dragging {
	background: url(images/line.gif) repeat-y 2px left;
	font-size: 12px;
}

.flexigrid div.iDiv {
	border: 1px solid #316ac5;
	position: absolute;
	overflow: visible;
	background: none;
	font-size: 12px;
}

.flexigrid div.iDiv input,.flexigrid div.iDiv select,.flexigrid div.iDiv textarea
	{
	font-family: Arial, Helvetica, sans-serif;
	font-size: 12px;
}

.flexigrid div.iDiv input.tb {
	border: 0px;
	padding: 0px;
	width: 100%;
	height: 100%;
	padding: 0px;
	background: none;
	font-size: 12px;
}

.flexigrid div.bDiv {
	border: 1px solid #d0d0d0;
	border-top: 0px;
	background: #fff;
	overflow: auto;
	position: relative;
	font-size: 12px;
}

.flexigrid div.bDiv table {
	border-bottom: 1px solid #d0d0d0;
	font-size: 12px;
}

.flexigrid div.hGrip {
	position: absolute;
	top: 0px;
	right: 0px;
	height: 5px;
	width: 5px;
	background: url(images/line.gif) repeat-x left;
	margin-right: 1px;
	cursor: col-resize;
	font-size: 12px;
}

.flexigrid div.hGrip:hover,.flexigrid div.hGrip.hgOver {
	border-right: 1px solid #999;
	margin-right: 0px;
	font-size: 12px;
}

.flexigrid div.hGrip.hgOverIE {
	border-right: 1px solid #999;
	margin-right: 1px;
	font-size: 12px;
}

.flexigrid div.vGrip {
	height: 4px;
	overflow: hidden;
	position: relative;
	background: #fafafa url(images/wbg2.gif) repeat-x 0px -1px;
	border: 1px solid #d0d0d0;
	border-top: 0px;
	text-align: center;
	cursor: row-resize;
}

.flexigrid div.vGrip span {
	display: block;
	margin: 1px auto;
	width: 20px;
	height: 1px;
	overflow: hidden;
	border-top: 1px solid #aaa;
	border-bottom: 1px solid #aaa;
	background: none;
}

.flexigrid div.hDiv th,.flexigrid  div.bDiv td
	/* common cell properties*/ {
	text-align: left;
	border-right: 1px solid #d0d0d0;
	border-left: 1px solid #fff;
	overflow: hidden;
	vertical-align: top !important;
}

.flexigrid div.hDiv th div,.flexigrid  div.bDiv td  div,div.colCopy div
	/* common inner cell properties*/ {
	padding: 5px;
	border-left: 0px solid #fff;
}

.flexigrid div.hDiv th,div.colCopy {
	font-weight: normal;
	height: 24px; /* 已选择列,标题的高度*/
	cursor: default;
	white-space: nowrap;
	overflow: hidden;
	font-weight: bold;
	color: #35455e;
}

div.colCopy {
	font-family: Arial, Helvetica, sans-serif;
	font-size: 12px;
	background: #fafafa url(images/fhbg.gif) repeat-x bottom;
	border: 1px solid #d0d0d0;
	border-bottom: 0px;
	overflow: hidden;
}

.flexigrid div.hDiv th.sorted {
	background: url(images/wbg.gif) repeat-x 0px -1px;
	border-bottom: 0px solid #d0d0d0;
}

.flexigrid div.hDiv th.thOver {
	
}

.flexigrid div.hDiv th.thOver div,.flexigrid div.hDiv th.sorted.thOver div
	{
	border-bottom: 1px solid orange;
	padding-bottom: 4px;
}

.flexigrid div.hDiv th.sorted div {
	border-bottom: 0px solid #d0d0d0;
	padding-bottom: 5px;
}

.flexigrid div.hDiv th.thMove {
	background: #fff;
	color: #fff;
}

.flexigrid div.hDiv th.sorted.thMove div {
	border-bottom: 1px solid #fff;
	padding-bottom: 4px
}

.flexigrid div.hDiv th.thMove div {
	background: #fff !important;
}

.flexigrid div.hDiv th div.sdesc {
	background: url(images/dn.png) no-repeat center top;
}

.flexigrid div.hDiv th div.sasc {
	background: url(images/up.png) no-repeat center top;
}

.flexigrid div.bDiv td {
	border-bottom: 1px solid #fff;
	vertical-align: top !important;
	white-space: nowrap;
	line-height: 20px;/* 兼容FF每行数据居中 */
}

.flexigrid div.hDiv th div {
	
}

.flexigrid span.cdropleft {
	display: block;
	background: url(images/prev.gif) no-repeat -4px left;
	width: 24px;
	height: 24px;
	position: relative;
	top: -24px;
	margin-bottom: -24px;
	z-index: 3;
}

.flexigrid div.hDiv span.cdropright {
	display: block;
	background: url(images/next.gif) no-repeat 12px left;
	width: 24px;
	height: 24px;
	float: right;
	position: relative;
	top: -24px;
	margin-bottom: -24px;
}

.flexigrid div.bDiv td div {
	border-top: 0px solid #fff;
	padding-bottom: 4px;
}

.flexigrid tr td.sorted {
	background: #f3f3f3;
	border-right: 1px solid #d0d0d0;
	border-bottom: 1px solid #f3f3f3;
}

.flexigrid tr td.sorted div {
	height: 22px;
	text-align: left;
}

.flexigrid tr.erow td {
	background: #f7f7f7;
	border-bottom: 1px solid #f7f7f7;
}

.flexigrid tr.erow td.sorted {
	background: #bae1f6;
	border-bottom: 1px solid #bae1f6;
}

.flexigrid tr.erow td.sorted div {
	
}

.flexigrid div.bDiv tr:hover td,.flexigrid div.bDiv tr:hover td.sorted,.flexigrid div.bDiv tr.trOver td.sorted,.flexigrid div.bDiv tr.trOver td
	{
	background: #e4f5a1;
	border-left: 1px solid #eef8ff;
	border-bottom: 1px dotted #a8d8eb;
}

.flexigrid div.bDiv tr.trSelected:hover td,.flexigrid div.bDiv tr.trSelected:hover td.sorted,.flexigrid div.bDiv tr.trOver.trSelected td.sorted,.flexigrid div.bDiv tr.trOver.trSelected td,.flexigrid tr.trSelected td.sorted,.flexigrid tr.trSelected td
	{
	background: #d5effc url(images/hl.png) repeat-x top;
	border-right: 1px solid #d2e3ec;
	border-left: 1px solid #eef8ff;
	border-bottom: 1px solid #a8d8eb;
}

/* novstripe adjustments */
.flexigrid.novstripe .bDiv table {
	border-bottom: 1px solid #d0d0d0;
	border-right: 1px solid #d0d0d0;
}

.flexigrid.novstripe  div.bDiv td {
	border-right-color: #fff;
}

.flexigrid.novstripe div.bDiv tr.erow td.sorted {
	border-right-color: #bae1f6;
}

.flexigrid.novstripe div.bDiv tr td.sorted {
	border-right-color: #f3f3f3;
}

.flexigrid.novstripe  div.bDiv tr.erow td {
	border-right-color: #f7f7f7;
	border-left-color: #f7f7f7;
}

.flexigrid.novstripe div.bDiv tr.trSelected:hover td,.flexigrid.novstripe div.bDiv tr.trSelected:hover td.sorted,.flexigrid.novstripe div.bDiv tr.trOver.trSelected td.sorted,.flexigrid.novstripe div.bDiv tr.trOver.trSelected td,.flexigrid.novstripe tr.trSelected td.sorted,.flexigrid.novstripe tr.trSelected td
	{
}

.flexigrid.novstripe div.bDiv tr.trOver td,.flexigrid.novstripe div.bDiv tr:hover td
	{
	border-left-color: #e4f5a1;
	border-right-color: #e4f5a1;
}

/* end novstripe */
.flexigrid div.pDiv {
	/*background: url(images/wbg.gif) repeat-x 0 -1px;*/
	border: 1px solid #d0d0d0;
	border-top: 0px;
	overflow: hidden;
	white-space: nowrap;
	position: relative;
}

.flexigrid div.pDiv div.pDiv2 {
	margin: 3px;
	margin-left: -2px;
	float: left;
	width: 1024px;
}

div.pGroup {
	float: left;
	background: none;
	height: 24px;
	margin: 0px 5px;
}

.flexigrid div.pDiv .pPageStat,.flexigrid div.pDiv .pcontrol {
	position: relative;
	top: 5px;
	overflow: visible;
}

.flexigrid div.pDiv input {
	vertical-align: text-top;
	position: relative;
	top: -5px;
}

.flexigrid div.pDiv  div.pButton {
	float: left;
	width: 22px;
	height: 22px;
	border: 0px;
	cursor: pointer;
	overflow: hidden;
}

.flexigrid div.pDiv  div.pButton:hover,.flexigrid div.pDiv  div.pButton.pBtnOver
	{
	width: 20px;
	height: 20px;
	border: 1px solid #d0d0d0;
	cursor: pointer;
}

.flexigrid div.pDiv  div.pButton.pBtnOverIE
	{
	width: 22px;
	height: 22px;
	border: 1px solid #d0d0d0;
	cursor: pointer;
}

.flexigrid div.pDiv  div.pButton span {
	width: 20px;
	height: 20px;
	display: block;
	float: left;
}

.flexigrid div.pDiv  div.pButton:hover span,.flexigrid div.pDiv  div.pButton.pBtnOver span
	{
	width: 19px;
	height: 19px;
	border-top: 1px solid #fff;
	border-left: 1px solid #fff;
}

.flexigrid .pSearch {
	background: url(images/magnifier.png) no-repeat left;
}

.flexigrid .pFirst {
	background: url(images/first.gif) no-repeat left;
}

.flexigrid .pPrev {
	background: url(images/prev.gif) no-repeat left;
}

.flexigrid .pNext {
	background: url(images/next.gif) no-repeat left;
}

.flexigrid .pLast {
	background: url(images/last.gif) no-repeat left;
}

.flexigrid .pReload {
	background: url(images/load.png) no-repeat left;
}

.flexigrid .pReload.loading {
	background: url(images/load.gif) no-repeat left;
}

/* ie adjustments */
.flexigrid.ie div.hDiv th div,.flexigrid.ie  div.bDiv td  div,div.colCopy.ie div
	/* common inner cell properties*/ {
	overflow: hidden;
}